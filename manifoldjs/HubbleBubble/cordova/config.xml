<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.elmarjansen.hubblebubble" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HubbleBubble</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="https://hubblebubble.elmarjansen.nl/" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="/images/homescreen48.png" />
        <icon density="hdpi" src="/images/homescreen72.png" />
        <icon density="xhdpi" src="/images/homescreen96.png" />
        <icon density="xxhdpi" src="/images/homescreen144.png" />
        <icon density="xxxhdpi" src="/images/homescreen192.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="72" src="/images/homescreen72.png" width="72" />
        <icon height="144" src="/images/homescreen144.png" width="144" />
    </platform>
    <preference name="Fullscreen" value="false" />
    <platform name="windows">
    </platform>
    <platform name="wp8">
    </platform>
    <allow-navigation hap-rule="yes" href="https://hubblebubble.elmarjansen.nl/*" />
</widget>
